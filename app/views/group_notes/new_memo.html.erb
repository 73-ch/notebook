<h1>メモの追加</h1>
<div id="form">
<div id="content">
<%= form_for(@group_note) do |f|%>
	<%= f.hidden_field :note_type , :value => "2"%>
	タイトル
	<br>
	<%= f.text_field :title %>
	<br>
	内容
	<br>
	<%= f.text_area :content %>
	<br>
	重要度
	<br>
	<%= f.range_field :importance %>
	<br>
	URL
	<br>
	<%= f.url_field :site_url %>
	<br>
	カテゴリー
	<%= f.collection_select :category_id, @categories, :id, :category_name %>
	<br>
	<input type="reset"　value="リセット">
	<%= f.submit "投稿" %>
<% end %>
<input type="button" onClick='history.back();' value="戻る">
</div>
</div>