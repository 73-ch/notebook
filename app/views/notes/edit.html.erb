<h1>メモの編集</h1>
<div id="form">
<%= form_for(@note) do |f|%>
	タイトル
	<br>
	<%= f.text_field :title %>
	<br>
	内容
	<br>
	<%= f.text_area :content %>
	<br>
	重要度
	<br>
	<%= f.range_field :importance %>
	<br>
	期間
	<br>
	<%= f.datetime_select :start_time %>〜
	<%= f.datetime_select :end_time %>
	<br>
	<input type="reset"　value="リセット">
	<%= f.submit "投稿" %>
<% end %>
<input type="button" onClick='history.back();' value="戻る">
</div>